{
  "name": "fast-submission2any",
  "version": "0.0.6",
  "description": "Generates an Excel file from an array of Form.io submissions",
  "main": "lib/fast-submission2any.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UN-FAO/fast-submission2any"
  },
  "keywords": [
    "form.io",
    "excel",
    "cordova",
    "browser",
    "browser excel"
  ],
  "author": "Ignacio Cabrera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UN-FAO/fast-submission2any/issues"
  },
  "homepage": "https://github.com/UN-FAO/fast-submission2any",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "fast-submission2csv": "0.0.18",
    "xlsx": "^0.12.12"
  }
}
